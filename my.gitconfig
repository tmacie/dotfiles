[alias]
	co = checkout
	st = status
	unstage = reset HEAD --
	uncommit = reset --soft HEAD~1
	comit = commit

[user]
	name = Tom Macie
	email = tmacie@gmail.com
[push]
	default = nothing
[merge]
	tool = kdiff3
[difftool]
	prompt = false
[mergetool]
	prompt = false
	tool = Kaleidoscope
[mergetool "kdiff3"]
	path = /Applications/kdiff3.app/Contents/MacOS/kdiff3
